<mvc:View                                                                                                                                                                                                                                                      
    xmlns:navpopover="sap.ui.comp.navpopover"                                                                                                                                                                                                                  
    xmlns:smartvariants="sap.ui.comp.smartvariants"                                                                                                                                                                                                            
    xmlns:f="sap.f"                                                                                                                                                                                                                                            
    controllerName="com.rds.rap.yrdsrap0020.controller.Main"                                                                                                                                                                                                   
    xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                
    displayBlock="true"                                                                                                                                                                                                                                        
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"                                                                                                                                                                                  
    xmlns="sap.m"                                                                                                                                                                                                                                              
    xmlns:core="sap.ui.core"                                                                                                                                                                                                                                   
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"                                                                                                                                                                                                          
    xmlns:smartTable="sap.ui.comp.smarttable"                                                                                                                                                                                                                  
>                                                                                                                                                                                                                                                              
    <f:DynamicPage id="Page" busy="{selected>/bBusy}" busyIndicatorDelay="0">                                                                                                                                                                                  
        <f:title>                                                                                                                                                                                                                                              
            <f:DynamicPageTitle>                                                                                                                                                                                                                               
                <f:content>                                                                                                                                                                                                                                    
                    <smartvariants:SmartVariantManagement id="PageSVM" entitySet="stock" showExecuteOnSelection="true" showShare="true" lifecycleSupport="true" useFavorites="true" initialSelectionKey="*standard*" persistencyKey="listReportFloorplanTable"+
 />                                                                                                                                                                                                                                                            
                </f:content>                                                                                                                                                                                                                                   
            </f:DynamicPageTitle>                                                                                                                                                                                                                              
        </f:title>                                                                                                                                                                                                                                             
        <f:header>                                                                                                                                                                                                                                             
            <f:DynamicPageHeader id="PageHeader">                                                                                                                                                                                                              
                <f:content>                                                                                                                                                                                                                                    
                    <smartFilterBar:SmartFilterBar id="PageSFB" entitySet="stock" useToolbar="false" considerAnalyticalParameters="true" useDateRangeType="true" persistencyKey="searchListReportVariant" />                                                   
                </f:content>                                                                                                                                                                                                                                   
            </f:DynamicPageHeader>                                                                                                                                                                                                                             
        </f:header>                                                                                                                                                                                                                                            
        <f:content>                                                                                                                                                                                                                                            
            <smartTable:SmartTable                                                                                                                                                                                                                             
                id="PageST"                                                                                                                                                                                                                                    
                entitySet="stock"                                                                                                                                                                                                                              
                smartFilterId="PageSFB"                                                                                                                                                                                                                        
                tableType="Table"                                                                                                                                                                                                                              
                showDetailsButton="true"                                                                                                                                                                                                                       
                showFullScreenButton="true"                                                                                                                                                                                                                    
                placeToolbarInTable="true"                                                                                                                                                                                                                     
                header="오더리스트"                                                                                                                                                                                                                                 
                demandPopin="true"                                                                                                                                                                                                                             
                enableAutoColumnWidth="true"                                                                                                                                                                                                                   
                detailsButtonSetting="None,Low,Medium"                                                                                                                                                                                                         
                exportType="UI5Client"                                                                                                                                                                                                                         
                initiallyVisibleFields="CustomerCreditAccount"                                                                                                                                                                                                 
                ignoredFields="CreditControlArea"                                                                                                                                                                                                              
                initialNoDataText="$FILTERBAR"                                                                                                                                                                                                                 
            >                                                                                                                                                                                                                                                  
                <smartTable:customToolbar>                                                                                                                                                                                                                     
                    <OverflowToolbar asyncMode="true">                                                                                                                                                                                                         
                        <ToolbarSpacer />                                                                                                                                                                                                                      
                        <Button id="btnRejectAll" text="{i18n>btnRejectAll}" press=".onPressRejectAll" enabled="{selected>/IsRowSelected}" />                                                                                                                  
                        <Button id="btnReprice" text="{i18n>btnReprice}" press=".onPressReprice" enabled="{selected>/IsRowSelected}" />                                                                                                                        
                    </OverflowToolbar>                                                                                                                                                                                                                         
                </smartTable:customToolbar>                                                                                                                                                                                                                    
                <Table id="tbMain"&nbsp;                                                                                                                                                                                                                       
                       mode="MultiSelect"&nbsp;                                                                                                                                                                                                                
                       alternateRowColors="true"&nbsp;                                                                                                                                                                                                         
                       growing="true"&nbsp;                                                                                                                                                                                                                    
                       selectionChange=".onSelectionChangeMain"&nbsp;                                                                                                                                                                                          
                       enableBusyIndicator="true"                                                                                                                                                                                                              
                       includeItemInSelection="false"                                                                                                                                                                                                          
                       itemPress="fcNavDetail">                                                                                                                                                                                                                
                    <headerToolbar />                                                                                                                                                                                                                          
                    <columns>                                                                                                                                                                                                                                  
                        <Column hAlign="Begin">                                                                                                                                                                                                                
                            <customData>                                                                                                                                                                                                                       
                                <core:CustomData key="p13nData" value='\{"columnKey": "SalesOrder", "autoColumnWidth": \{ "visibleProperty": "SalesOrder" }, "columnIndex": 2, "leadingProperty": "SalesOrder", "sortProperty": "SalesOrder"}' />              
                            </customData>                                                                                                                                                                                                                      
                            <Text text="{/#stockType/SalesOrder/@sap:label}" />                                                                                                                                                                                
                        </Column>                                                                                                                                                                                                                              
						 <Column hAlign="Begin">                                                                                                                                                                                                                                 
                            <customData>                                                                                                                                                                                                                       
                                <core:CustomData key="p13nData" value='\{"columnKey": "SoldToPartyName", "autoColumnWidth": \{ "visibleProperty": "SoldToPartyName" }, "columnIndex": 12, "leadingProperty": "SoldToPartyName", "sortProperty": "SoldToPartyNa+
me"}' />                                                                                                                                                                                                                                                       
                            </customData>                                                                                                                                                                                                                      
                            <Text text="{/#stockType/SoldToPartyName/@sap:label}" />                                                                                                                                                                           
                        </Column>                                                                                                                                                                                                                              
						 <Column hAlign="Begin">                                                                                                                                                                                                                                 
                            <customData>                                                                                                                                                                                                                       
                                <core:CustomData key="p13nData" value='\{"columnKey": "CustomerCreditAccountName", "autoColumnWidth": \{ "visibleProperty": "CustomerCreditAccountName" }, "columnIndex": 13, "CustomerCreditAccountName": "SalesOrder", "sort+
Property": "CustomerCreditAccountName"}' />                                                                                                                                                                                                                    
                            </customData>                                                                                                                                                                                                                      
                            <Text text="{/#stockType/CustomerCreditAccountName/@sap:label}" />                                                                                                                                                                 
                        </Column>                                                                                                                                                                                                                              
                    </columns>                                                                                                                                                                                                                                 
                    <items>                                                                                                                                                                                                                                    
                        <ColumnListItem type="Navigation">                                                                                                                                                                                                     
                            <cells>                                                                                                                                                                                                                            
                                <Link app:Semantic="SalesOrder" app:Action="display" text="{SalesOrder}" press="pressLinkExternal" />                                                                                                                          
								<Link app:Semantic="SPOC09" app:Action="F0850A" text="{SoldToPartyName}" press="pressLinkExternal" />                                                                                                                                                  
								<Link app:Semantic="SPOC09" app:Action="F4596" text="{CustomerCreditAccountName}" press="pressLinkExternal" />                                                                                                                                         
                            </cells>                                                                                                                                                                                                                           
                        </ColumnListItem>                                                                                                                                                                                                                      
                    </items>                                                                                                                                                                                                                                   
                </Table>                                                                                                                                                                                                                                       
            </smartTable:SmartTable>                                                                                                                                                                                                                           
        </f:content>                                                                                                                                                                                                                                           
    </f:DynamicPage>                                                                                                                                                                                                                                           
</mvc:View>                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               