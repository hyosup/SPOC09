<c:FragmentDefinition                                                                                                                                                                                                                                          
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:c="sap.ui.core"                                                                                                                                                                                                                                         
>                                                                                                                                                                                                                                                              
	<Dialog                                                                                                                                                                                                                                                       
		id="dialogRepriceResult"                                                                                                                                                                                                                                     
		title="{i18n>tlReprice}"                                                                                                                                                                                                                                     
		state="{selected>/sState}"                                                                                                                                                                                                                                   
		contentHeight="50%"                                                                                                                                                                                                                                          
		verticalScrolling="false"                                                                                                                                                                                                                                    
	>                                                                                                                                                                                                                                                             
		<content>                                                                                                                                                                                                                                                    
			<MessageView                                                                                                                                                                                                                                                
				id="repResMV"                                                                                                                                                                                                                                              
				items="{repriceMessage>/}"                                                                                                                                                                                                                                 
				groupItems="true"                                                                                                                                                                                                                                          
			>                                                                                                                                                                                                                                                           
				<items>                                                                                                                                                                                                                                                    
					<MessageItem                                                                                                                                                                                                                                              
						type="{repriceMessage>type}"                                                                                                                                                                                                                             
						title="{repriceMessage>message}"                                                                                                                                                                                                                         
						groupName="{repriceMessage>description}"                                                                                                                                                                                                                 
					/>                                                                                                                                                                                                                                                        
				</items>                                                                                                                                                                                                                                                   
			</MessageView>                                                                                                                                                                                                                                              
		</content>                                                                                                                                                                                                                                                   
		<buttons>                                                                                                                                                                                                                                                    
			<Button                                                                                                                                                                                                                                                     
				id="btnCloseRepriceResult"                                                                                                                                                                                                                                 
				text="{i18n>btnClose}"                                                                                                                                                                                                                                     
				press="onCloseRepRes"                                                                                                                                                                                                                                      
			/>                                                                                                                                                                                                                                                          
		</buttons>                                                                                                                                                                                                                                                   
		<endButton>                                                                                                                                                                                                                                                  
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</c:FragmentDefinition>                                                                                                                                                                                                                                        