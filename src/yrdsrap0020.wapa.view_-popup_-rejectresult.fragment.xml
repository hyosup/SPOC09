<c:FragmentDefinition                                                                                                                                                                                                                                          
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:c="sap.ui.core"                                                                                                                                                                                                                                         
>                                                                                                                                                                                                                                                              
	<Dialog                                                                                                                                                                                                                                                       
		id="dialogRejectResult"                                                                                                                                                                                                                                      
		title="{i18n>tlRejectAll}"                                                                                                                                                                                                                                   
		state="{selected>/sState}"                                                                                                                                                                                                                                   
		contentHeight="50%"                                                                                                                                                                                                                                          
		verticalScrolling="false"                                                                                                                                                                                                                                    
	>                                                                                                                                                                                                                                                             
		<content>                                                                                                                                                                                                                                                    
			<MessageView                                                                                                                                                                                                                                                
				id="rejResMV"                                                                                                                                                                                                                                              
				items="{rejectMessage>/}"                                                                                                                                                                                                                                  
				groupItems="true"                                                                                                                                                                                                                                          
			>                                                                                                                                                                                                                                                           
				<items>                                                                                                                                                                                                                                                    
					<MessageItem                                                                                                                                                                                                                                              
						type="{rejectMessage>type}"                                                                                                                                                                                                                              
						title="{rejectMessage>message}"                                                                                                                                                                                                                          
						groupName="{rejectMessage>description}"                                                                                                                                                                                                                  
					/>                                                                                                                                                                                                                                                        
				</items>                                                                                                                                                                                                                                                   
			</MessageView>                                                                                                                                                                                                                                              
		</content>                                                                                                                                                                                                                                                   
		<buttons>                                                                                                                                                                                                                                                    
			<Button                                                                                                                                                                                                                                                     
				id="btnCloseRejectResult"                                                                                                                                                                                                                                  
				text="{i18n>btnClose}"                                                                                                                                                                                                                                     
				press="onCloseRejRes"                                                                                                                                                                                                                                      
			/>                                                                                                                                                                                                                                                          
		</buttons>                                                                                                                                                                                                                                                   
		<endButton>                                                                                                                                                                                                                                                  
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</c:FragmentDefinition>                                                                                                                                                                                                                                        