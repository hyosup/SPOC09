<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:core="sap.ui.core"                                                                                                                                                                                                                                      
>                                                                                                                                                                                                                                                              
	<Dialog                                                                                                                                                                                                                                                       
		id="dialogReprice"                                                                                                                                                                                                                                           
		title="{i18n>tlReprice}"                                                                                                                                                                                                                                     
	>                                                                                                                                                                                                                                                             
		<beginButton>                                                                                                                                                                                                                                                
			<Button                                                                                                                                                                                                                                                     
				text="{i18n>btnReprice}"                                                                                                                                                                                                                                   
				type="Emphasized"                                                                                                                                                                                                                                          
				press=".onReprice"                                                                                                                                                                                                                                         
			/>                                                                                                                                                                                                                                                          
		</beginButton>                                                                                                                                                                                                                                               
		<content>                                                                                                                                                                                                                                                    
			<VBox                                                                                                                                                                                                                                                       
				height="auto"                                                                                                                                                                                                                                              
				width="90%"                                                                                                                                                                                                                                                
				class="sapUiSmallMargin"                                                                                                                                                                                                                                   
			>                                                                                                                                                                                                                                                           
				<Text text="{i18n>fdPricingDate}:"/>                                                                                                                                                                                                                       
				<DatePicker                                                                                                                                                                                                                                                
					id="repriceDP"                                                                                                                                                                                                                                            
					valueFormat="yyyy-MM-dd"                                                                                                                                                                                                                                  
					displayFormat="MM/dd/yyyy"                                                                                                                                                                                                                                
				/>                                                                                                                                                                                                                                                         
			</VBox>                                                                                                                                                                                                                                                     
		</content>                                                                                                                                                                                                                                                   
		<endButton>                                                                                                                                                                                                                                                  
			<Button                                                                                                                                                                                                                                                     
				text="{i18n>btnCanc}"                                                                                                                                                                                                                                      
				press=".onCloseRep"                                                                                                                                                                                                                                        
			/>                                                                                                                                                                                                                                                          
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     