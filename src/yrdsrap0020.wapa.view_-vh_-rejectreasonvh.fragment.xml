<c:FragmentDefinition                                                                                                                                                                                                                                          
	xmlns:fb="sap.ui.comp.filterbar"                                                                                                                                                                                                                              
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:c="sap.ui.core"                                                                                                                                                                                                                                         
	xmlns:vh="sap.ui.comp.valuehelpdialog"                                                                                                                                                                                                                        
>                                                                                                                                                                                                                                                              
	<vh:ValueHelpDialog                                                                                                                                                                                                                                           
		id="vhDialogRejectReason"                                                                                                                                                                                                                                    
		title="{i18n>tlRejectReason}"                                                                                                                                                                                                                                
		ok=".onSelectionChangeRejRsnVH"                                                                                                                                                                                                                              
		cancel=".onVHCancRejRsn"                                                                                                                                                                                                                                     
		selectionChange=".onSelectionChangeRejRsnVH"                                                                                                                                                                                                                 
		supportMultiselect="false"                                                                                                                                                                                                                                   
		contentHeight="auto"                                                                                                                                                                                                                                         
	>                                                                                                                                                                                                                                                             
		<vh:filterBar>                                                                                                                                                                                                                                               
			<fb:FilterBar                                                                                                                                                                                                                                               
				id="fbRejectReason"                                                                                                                                                                                                                                        
				search=".valuHelpCustomFilter_rejectAll"                                                                                                                                                                                                                   
				showFilterConfiguration="false"                                                                                                                                                                                                                            
			>                                                                                                                                                                                                                                                           
				<fb:filterGroupItems>                                                                                                                                                                                                                                      
					<fb:FilterGroupItem                                                                                                                                                                                                                                       
						groupName="__$INTERNAL$"                                                                                                                                                                                                                                 
						name="rejectAllReason"                                                                                                                                                                                                                                   
						label="{i18n>fdRejectAllReason}"                                                                                                                                                                                                                         
						visibleInFilterBar="true"                                                                                                                                                                                                                                
					>                                                                                                                                                                                                                                                         
						<fb:control>                                                                                                                                                                                                                                             
							<MultiInput                                                                                                                                                                                                                                             
								id="MIVHRejRsn"                                                                                                                                                                                                                                        
								maxTokens="1"                                                                                                                                                                                                                                          
								name="Status"                                                                                                                                                                                                                                          
								change="valuHelpCustomFilter_rejectAll"                                                                                                                                                                                                                
								tokenUpdate="valuHelpCustomFilter_rejectAll"                                                                                                                                                                                                           
								showValueHelp="true"                                                                                                                                                                                                                                   
								valueHelpRequest="onReasonVH"                                                                                                                                                                                                                          
							/>                                                                                                                                                                                                                                                      
						</fb:control>                                                                                                                                                                                                                                            
					</fb:FilterGroupItem>                                                                                                                                                                                                                                     
					<fb:FilterGroupItem                                                                                                                                                                                                                                       
						groupName="__$INTERNAL$"                                                                                                                                                                                                                                 
						name="rejectAllReasonDescription"                                                                                                                                                                                                                        
						label="{i18n>fdDescription}"                                                                                                                                                                                                                             
						visibleInFilterBar="true"                                                                                                                                                                                                                                
					>                                                                                                                                                                                                                                                         
						<fb:control>                                                                                                                                                                                                                                             
							<MultiInput                                                                                                                                                                                                                                             
								id="MIVHRejRsnDes"                                                                                                                                                                                                                                     
								maxTokens="1"                                                                                                                                                                                                                                          
								name="StatusDesc"                                                                                                                                                                                                                                      
								change="valuHelpCustomFilter_rejectAll"                                                                                                                                                                                                                
								tokenUpdate="valuHelpCustomFilter_rejectAll"                                                                                                                                                                                                           
								showValueHelp="true"                                                                                                                                                                                                                                   
								valueHelpRequest="onDescriptionVH"                                                                                                                                                                                                                     
							/>                                                                                                                                                                                                                                                      
						</fb:control>                                                                                                                                                                                                                                            
					</fb:FilterGroupItem>                                                                                                                                                                                                                                     
				</fb:filterGroupItems>                                                                                                                                                                                                                                     
			</fb:FilterBar>                                                                                                                                                                                                                                             
		</vh:filterBar>                                                                                                                                                                                                                                              
	</vh:ValueHelpDialog>                                                                                                                                                                                                                                         
</c:FragmentDefinition>                                                                                                                                                                                                                                        