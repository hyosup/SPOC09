<c:FragmentDefinition                                                                                                                                                                                                                                          
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:vh="sap.ui.comp.valuehelpdialog"                                                                                                                                                                                                                        
	xmlns:c="sap.ui.core"                                                                                                                                                                                                                                         
>                                                                                                                                                                                                                                                              
	<vh:ValueHelpDialog                                                                                                                                                                                                                                           
		id="dialogDescription"                                                                                                                                                                                                                                       
		title="{i18n>tlDescription}"                                                                                                                                                                                                                                 
		ok=".onDescriptionVHOK"                                                                                                                                                                                                                                      
		cancel=".onDescriptionVHCanc"                                                                                                                                                                                                                                
		afterClose=".onDescriptionVHClose"                                                                                                                                                                                                                           
		key="StatusDesc"                                                                                                                                                                                                                                             
		descriptionKey="{i18n>fdDescription}"                                                                                                                                                                                                                        
		supportRanges="true"                                                                                                                                                                                                                                         
		supportRangesOnly="true"                                                                                                                                                                                                                                     
		maxIncludeRanges="1"                                                                                                                                                                                                                                         
		maxExcludeRanges="0"                                                                                                                                                                                                                                         
		maxConditions="1"                                                                                                                                                                                                                                            
	/>                                                                                                                                                                                                                                                            
</c:FragmentDefinition>                                                                                                                                                                                                                                        